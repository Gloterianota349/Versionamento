programa
{
	funcao inicio()
{
		inteiro x, y
	
		escreva("Digite dois valores inteiros: ")
		leia(x, y)
		se(x>y){
			escreva(x, " ", y)
		}
		senao se(x<y){
			escreva(y, " ", x)
		}
		senao{
			escreva(x, " e ", y, " são iguais")
		}
	}
}
